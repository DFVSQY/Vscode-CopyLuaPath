{
	"name": "copyluapath",
	"publisher": "DFVSQY",
	"displayName": "CopyLuaPath",
	"description": "This is an extension that implemente copy lua path according to my idea",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:copyluapath.copyPath"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "copyluapath.copyPath",
				"title": "Copy Lua Path"
			}
		],
		"configuration":[
			{
				"title": "CopyLuaPath",
				"properties": {
					"copyluapath.luaPathPrefix": {
						"type":"string",
						"default":"Assets/Scripts/Lua/",
						"description": "Lua prefix path that will be ignore"
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.44.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.26.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
